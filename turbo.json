{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"outputs": ["dist/**", ".next/**", "build/**"],
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false
		},
		"lint": {
			"cache": false,
			"outputs": []
		},
		"lint:fix": {
			"cache": false,
			"outputs": []
		}
	}
}
